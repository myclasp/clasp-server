$('#popup-<%= @moment.id %> .possible-feature').replaceWith('<%= j render(partial: "feature", locals: { moment: @moment }) %>');

<% if current_user and moment.user.eql?(current_user) and moment.has_feature.nil? %>
  MomentMap.reverseGeocode('#popup-<%= @moment.id %>', <%= @moment.latlng %>);
<% end %>